  "scripts": {
    "build": "npm run js-build && npm run sass && eleventy",
    "watch": "concurrently \"eleventy --serve\" \"npm run js-build-watch\" \"npm run sass-watch\"",
    "serve": "eleventy --serve",
    "js-build": "rollup -c rollup.config.js",
    "js-build-watch": "rollup -c rollup.config.js -w",
    "debug": "DEBUG=* eleventy",
    "sass": "sass scss/main.scss css/main.css",
    "sass-watch": "spn run sass -- --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "build"
    }
  },
